{
  "name": "amazeing",
  "version": "1.0.0",
  "description": "A map generator utilizing generators.",
  "main": "./dist/index.js",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "parcel build src/index.ts --target node",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellyirc/amazeing.git"
  },
  "keywords": [
    "maze",
    "generator",
    "map"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyirc/amazeing/issues"
  },
  "homepage": "https://github.com/kellyirc/amazeing#readme",
  "dependencies": {
    "seedrandom": "^2.4.4"
  },
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "parcel-bundler": "^1.10.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
